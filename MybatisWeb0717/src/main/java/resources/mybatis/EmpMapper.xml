<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.mybatis.persistence.EmpMapper">
    <select id="list" resultType="java.util.Map">
        SELECT list
        FROM trip
    </select>
    <insert id="ins_emp_temp" parameterType="com.db.Emp">
    	insert into trip(seqnum, list, regdate)
    	values(#{seq_trip.nextval}, #{list}, #{regdate})
    </insert>
    <update id="upd_emp_temp" parameterType="com.db.Emp">
    	update trip
    	set list = #{list}
    	      regdate = #{regdate}
    	where seqnum = #{seqnum}
    </update>
    <delete id="del_emp_temp" parameterType="com.db.Emp">
    	delete trip
    	where seqnum = #{seqnum}
    </delete>
</mapper>
